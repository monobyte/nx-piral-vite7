{
  "name": "@proj/grid",
  "version": "1.0.0",
  "private": true,
  "description": "Pre-configured AG-Grid components and styles",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "source": "src/index.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./styles": {
      "import": "./dist/styles.css",
      "require": "./dist/styles.css"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean && npm run build:styles",
    "build:styles": "cp src/styles.css dist/styles.css",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "lint": "eslint src --ext .ts,.tsx"
  },
  "dependencies": {
    "ag-grid-community": "^32.3.0",
    "ag-grid-react": "^32.3.0"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5"
  },
  "nx": {
    "targets": {
      "build": {
        "outputs": ["{projectRoot}/dist"]
      }
    }
  }
}
